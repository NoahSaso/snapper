{
  "name": "snapper",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "lint": "eslint .",
    "format": "eslint . --fix",
    "test": "NODE_ENV=test node --nolazy --inspect=0.0.0.0:9227 ./node_modules/.bin/jest --runInBand",
    "test:docker": "docker compose -f compose.test.yml up --exit-code-from test",
    "build": "tsc && tsc-alias",
    "serve": "node dist/server/app.js",
    "revalidate": "node dist/scripts/revalidate.js",
    "start": "pm2 start ecosystem.config.js --no-daemon",
    "start:dev": "docker compose -f compose.dev.yml up --exit-code-from app",
    "start:prod": "pm2 start ecosystem.config.js"
  },
  "license": "AGPL-3.0-only",
  "devDependencies": {
    "@types/busboy": "^1.5.3",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.11.10",
    "@types/range-parser": "^1.2.7",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.7.0",
    "pm2": "^5.3.1",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.7",
