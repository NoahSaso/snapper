{
  "name": "snapper",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "postinstall": "patch-package",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "test": "vitest",
    "test:docker": "docker compose -f compose.test.yml up --exit-code-from test",
    "build": "tsc && tsup",
    "build:only": "tsup",
    "build:watch": "tsup --watch",
    "serve": "node dist/server/app.js",
    "serve:dev": "npm run build:watch -- --onSuccess 'npm run serve'",
    "process": "node dist/queues/process.js",
    "flushQuery": "node dist/scripts/flushQuery.js",
    "flushMatching": "node dist/scripts/flushMatching.js",
    "generateKeyPair": "node dist/scripts/generateKeyPair.js",
    "generateKeyPair:default": "node dist/scripts/generateKeyPair.js -f key",
    "start": "pm2-runtime start ecosystem.config.js",
    "start:dev": "docker compose -f compose.dev.yml up",
    "start:prod": "pm2 reload ecosystem.config.js",
    "docker:build": "docker buildx build --platform linux/amd64 -t snapper ."
  },
  "license": "AGPL-3.0-only",
  "devDependencies": {
    "@chain-registry/types": "^0.50.185",
    "@dao-dao/config": "^2.7.1-rc.4",
    "@swc/core": "^1.4.16",
    "@types/busboy": "^1.5.3",
    "@types/cors": "^2.8.17",
    "@types/lodash.uniq": "^4.5.9",
    "@types/node": "^20.11.10",
    "@types/pako": "^2.0.3",
    "@types/range-parser": "^1.2.7",
    "@types/safe-compare": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.0.0",
    "patch-package": "^8.0.0",
    "pm2": "^5.3.1",
    "prettier": "^3.2.4",
    "tsup": "^8.0.2",
    "typescript": "5.3.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@apollo/client": "^3.8.7",
    "@bull-board/fastify": "^5.19.0",
    "@cosmjs/cosmwasm-stargate": "^0.33.1",
    "@cosmjs/encoding": "^0.33.1",
    "@cosmjs/proto-signing": "^0.33.1",
    "@cosmjs/stargate": "^0.33.1",
    "@cosmjs/tendermint-rpc": "^0.33.1",
    "@dao-dao/state": "^2.7.1-rc.4",
    "@dao-dao/types": "^2.7.1-rc.4",
    "@dao-dao/utils": "^2.7.1-rc.4",
    "@fastify/basic-auth": "^5.1.1",
    "@fastify/cors": "^9.0.1",
    "@skip-go/client": "^0.16.4",
    "axios": "^1.6.7",
    "bullmq": "^5.7.12",
    "commander": "^12.0.0",
    "dotenv": "^16.4.4",
    "ethers": "^6.13.4",
    "fastify": "^4.27.0",
    "ioredis": "^5.3.2",
    "json-stringify-deterministic": "^1.0.12",
    "lodash.uniq": "^4.5.0",
    "meilisearch": "^0.40.0",
    "safe-compare": "^1.1.4"
  }
}
